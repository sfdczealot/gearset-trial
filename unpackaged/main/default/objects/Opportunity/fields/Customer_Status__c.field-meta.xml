<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Customer_Status__c</fullName>
    <description>If sequential liability is true then both the media agency and the advertiser need to pass account approval.
Else the advertiser only need to pass the clean step.</description>
    <externalId>false</externalId>
    <formula>IF(
   Account.Account_Status__c == &#39;APPROVED&#39;,
   &quot;Approved&quot;,
   &quot;Check Account&quot;
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Customer Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
