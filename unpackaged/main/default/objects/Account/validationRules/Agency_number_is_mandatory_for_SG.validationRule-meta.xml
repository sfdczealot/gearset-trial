<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Agency_number_is_mandatory_for_SG</fullName>
    <active>true</active>
    <errorConditionFormula>and (ISBLANK(CEA_License__c),
					ISPICKVAL(Country__c , &#39;Singapore&#39;),
					RecordType.DeveloperName = &#39;Agency&#39;)</errorConditionFormula>
    <errorMessage>Agency Identification Number is mandatory</errorMessage>
</ValidationRule>
