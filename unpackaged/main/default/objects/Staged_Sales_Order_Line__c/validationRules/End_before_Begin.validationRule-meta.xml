<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_before_Begin</fullName>
    <active>true</active>
    <errorConditionFormula>Flight_End__c &lt; Flight_Begin__c</errorConditionFormula>
    <errorDisplayField>Flight_Begin__c</errorDisplayField>
    <errorMessage>The Flight Begin must be before Flight End</errorMessage>
</ValidationRule>
