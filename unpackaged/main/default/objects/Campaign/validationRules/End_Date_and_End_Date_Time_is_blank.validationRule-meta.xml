<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_and_End_Date_Time_is_blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND(RecordType.Name=&quot;Marketing Promo&quot;,OR(ISBLANK(EndDate),ISBLANK(Campaign_End_DateTime__c)))</errorConditionFormula>
    <errorMessage>End Date and End Date Time should not be blank.</errorMessage>
</ValidationRule>
